    <section class="u-align-center u-clearfix u-section-1" id="sec-40be">
        <div class="u-clearfix u-sheet u-valign-middle u-sheet-1">
            <h2 class="u-text u-text-default u-text-grey-80 u-text-1">Sign Up</h2>
            <div class="u-form u-form-1">
                

                <form method="post" asp-action="AddUser" asp-controller="SignUp"  style="padding: 15px;" source="email" name="form">
                        <div class="u-form-group u-label-none">
                            <label for="text-dce3" class="u-label">Поле введення</label>
                            <input type="text" required placeholder="Enter your Name" id="text-dce3" name="name" class="u-border-1 u-border-grey-30 u-input u-input-rectangle" asp-for="Name">
                        </div>

                        <div class="u-form-email u-form-group u-label-none">
                            <label for="email-6797" class="u-label">Email</label>
                            <input type="email" required asp-for="Email" placeholder="Enter your Email" id="email-6797" name="email" class="u-border-1 u-border-grey-30 u-input u-input-rectangle" required>
                        </div>

                        <div class="u-form-group u-label-none">
                            <label for="text-2ad1" class="u-label">Поле введення</label>
                            <input type="password" required asp-for="Password" placeholder="Enter your Password" id="text-2ad1" name="password" class="u-border-1 u-border-grey-30 u-input u-input-rectangle">
                        </div>

                        <div class="u-border-3 u-border-grey-dark-1 u-form-group u-form-line u-line u-line-horizontal u-line-1"></div>


                        <div class="u-align-left u-form-group u-form-submit">
                        <a href="" class="u-btn u-btn-submit u-button-style" >Submit</a>
                            <input type="submit" value="submit" class="u-form-control-hidden">
                        </div>
            
                </form>

            </div>

            @if(TempData["MessageForSignUp"] == "OK")
            {
                <q class="u-text u-text-default u-text-2">
                    Successfully added
                </q>
                <a href="/home/index" class="u-btn u-button-style u-hover-palette-1-dark-1 u-palette-1-base u-btn-2">Home</a>
            }
            else if(TempData["MessageForSignUp"] == "ERROR")
            {
                <q class="u-text u-text-default u-text-2">
                    A user with this email already exists
                </q>
                <a href="/home/index" class="u-btn u-button-style u-hover-palette-1-dark-1 u-palette-1-base u-btn-2">Home</a>
            }


        </div>
   
    
    </section>


